{
  "npmScope": "bghoard",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": []
    },
    "api-interfaces": {
      "tags": []
    },
    "review": {
      "tags": []
    },
    "review-data-access-games": {
      "tags": []
    },
    "review-e2e": {
      "tags": [],
      "implicitDependencies": ["review"]
    },
    "review-feature-details": {
      "tags": []
    },
    "review-feature-list": {
      "tags": []
    },
    "review-ui-tile": {
      "tags": []
    },
    "review-utils-formatters": {
      "tags": []
    }
  }
}
